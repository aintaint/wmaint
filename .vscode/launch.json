    "version": "0.2.0",
    "configurations": [{
        "name": ".NET Core Launch (console)",
        "type": "coreclr",
        "request": "launch",
        "preLaunchTask": "build",
        // if target frameworks was changed, update program path.
        "program": 
          "${workspaceFolder}/bin/Debug/net8.0-windows10.0.19041.0/win-x64/wmaint.exe",
        "args": [],
        "cwd": "${workspaceFolder}",
          // 'console' field https://aka.ms/VSCode-CS-LaunchJson-Console
        "console": "internalConsole",
        "stopAtEntry": false
      },
      {
        "name": ".NET Core Attach",
        "type": "coreclr",
        "request": "attach"
      }]
}
